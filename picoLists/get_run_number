# pp500 2017
get_file_list.pl -keys 'runnumber' -cond 'production=P20ic,library=SL22b,trgsetupname=pp500_production_2017,filetype=daq_reco_picoDst,filename~st_physics,storage=local' -limit 0 -delim / -distinct > runs_numbers.list